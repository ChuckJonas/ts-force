{
  "name": "ts-force-gen",
  "version": "3.2.0",
  "description": "Code generation for ts-force",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "bin": {
    "ts-force-gen": "build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChuckJonas/ts-force.git"
  },
  "keywords": [
    "salesforce",
    "typescript",
    "rest"
  ],
  "contributors": [
    "Charlie Jonas"
  ],
  "license": "BSD-3-Clause",
  "scripts": {
    "clean": "rm -r -f ./build",
    "clean-build": "npm run clean && npm run build",
    "link": "npm run clean-build && npm link",
    "pretest": "npm run clean && tsc",
    "test": "mocha --require ts-node/register test/**/*.spec.ts ",
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc -p tsconfig.build.json -w",
    "generate-json-schema": "npx typescript-json-schema --required ./tsconfig.json Config > ./ts-force-config.schema.json",
    "prepublishOnly": "npm run clean-build && npm run generate-json-schema"
  },
  "engines": {
    "node": ">=4.5"
  },
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/cli-spinner": "^0.2.0",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^8.2.2",
    "@types/nock": "^8.2.1",
    "@types/node": "^14.11.4",
    "chai": "^4.1.2",
    "mocha": "^8.3.2",
    "mocha-typescript": "^1.1.8",
    "nock": "^9.2.6",
    "source-map-support": "^0.4.18",
    "ts-mocha": "^8.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.4.3",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^4.0.3",
    "typescript-json-schema": "^0.43.0"
  },
  "dependencies": {
    "@salesforce/core": "^2.12.3",
    "cli-spinner": "0.2.10",
    "dotenv": "^4.0.0",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "ts-morph": "^2.1.0",
    "tslib": "^2.0.3"
  }
}
